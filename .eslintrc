{
  "extends": [
    "airbnb"
  ],
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "legacyDecorators": true
    },
    "sourceType": "module"
  },
  "rules": {
    "react/destructuring-assignment": false
  },
  "settings": {
    "import/resolver": "webpack"
  }
}
