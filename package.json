{
  "name": "react-mobx-boilerplate",
  "version": "0.0.0",
  "description": "boilerplate for a react + mobx application",
  "main": "./src/app.jsx",
  "scripts": {
    "dev": "webpack-dev-server",
    "build": "webpack --display-error-details --progress --colors --mode=production",
