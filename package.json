{
  "name": "react-mobx-boilerplate",
  "version": "0.0.0",
  "description": "boilerplate for a react + mobx application",
  "main": "./src/app.jsx",
  "scripts": {
    "dev": "webpack-dev-server --hot",
    "build": "webpack --display-error-details --progress --colors --mode=production --env.NODE_ENV=production",
    "lint": "eslint --ext .js,.jsx --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext .js,.jsx --ignore-path .gitignore .",
    "precommit": "lint-staged",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RomanKol/react-mobx-boilerplate.git"
  },
  "keywords": [
    "react",
    "mobx",
    "boilerplate"
  ],
  "author": "Roman Kollatschny <mail@kollatschny.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RomanKol/react-mobx-boilerplate/issues"
  },
  "homepage": "https://github.com/RomanKol/react-mobx-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.1.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-emotion": "^9.2.11",
    "babel-preset-env": "^1.7.0",
    "babel-preset-mobx": "^2.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.3",
    "image-webpack-loader": "^4.5.0",
    "lint-staged": "^8.0.4",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "emotion": "^9.2.12",
    "mobx": "^5.6.0",
    "mobx-react": "^5.3.6",
    "prop-types": "^15.6.2",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-emotion": "^9.2.12",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.jsx": "eslint"
  }
}
